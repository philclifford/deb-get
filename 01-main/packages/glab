get_gitlab_releases "gitlab-org%2Fcli" "latest"

if [ "${ACTION}" != "prettylist" ]; then
    #URL=$(wget  -q -O- "https://gitlab.com/api/v4/projects/gitlab-org%2Fcli/releases/permalink/latest/assets/links" | jq -r '.[] |select(.name|contains("x86_64.deb"))| .direct_asset_url')
    URL=$(jq <${CACHE_FILE} -r '.[] |select(.name|contains("x86_64.deb"))| .direct_asset_url')
    VERSION_PUBLISHED="${URL##*/releases/v}"
    VERSION_PUBLISHED=${VERSION_PUBLISHED%%/*}
fi

